class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        stack = []
        flag = True
        for i in s:
            if i in ('(', '{', '['):
                stack.append(i)
            elif stack:
                top = stack.pop()
                comb = top + i
                if comb not in ('()', '{}', '[]'):
                    return False
            else:
                return False
        if stack:
            return False
        return True